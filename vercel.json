{
    "version": 2,
    "builds": [
      {
        "src": "src/main.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { 
        "handle": "src/main.ts", 
        "src": "/(.*)", 
        "dest": "/src/main.ts",
        "methods" : [
            "GET",
            "POST",
            "PUT",
            "DELETE"

        ]
      }
    ]
  }
  